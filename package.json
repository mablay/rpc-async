{
  "name": "rpc-async",
  "version": "0.3.0",
  "description": "Augments async communication with RPC functionality (ws, events, ipc, ...)",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=neutral --outfile=index.js",
    "test": "tsx --test-reporter spec --test test/*.test.ts",
    "cloc": "npx clock index.js",
    "coverage": "npx c8 pta",
    "packlist": "npx npm-packlist",
    "deploy": "./publish.sh",
    "cluster": "esbuild example/cluster.ts --bundle --format=esm --platform=node --outdir=build && node build/cluster.js"
  },
  "dependencies": {
    "@occami/events": "^0.1.16"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "esbuild": "^0.15.13",
    "node": "^19.8.1",
    "pta": "^1.0.2",
    "tsx": "^3.12.7",
    "typescript": "^4.8.3",
    "ws": "^8.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mablay/rpc-async.git"
  },
  "keywords": [
    "rpc",
    "async",
    "es6"
  ],
  "author": "marc.ablay@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mablay/rpc-async/issues"
  },
  "homepage": "https://github.com/mablay/rpc-async#readme",
  "typings": "index.d.ts"
}
